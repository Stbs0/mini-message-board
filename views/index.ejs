<div>
  <h1>Mini Message Board</h1>
  <div>
    <% for (let i = 0 ; i < messages.length; i++) { %>
    <p><%= messages[i].author %> : <%= messages[i].text %> , added: <%= messages[i].added %></p>
    <a href="/<%= messages[i].author %>"><button onclick="">open</button></a>
    <hr />
    <% } %>
  </div>
  <a href="/new">add message</a>
</div>
